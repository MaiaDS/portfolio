{
    "extends": [
        "next/core-web-vitals",
        "standard-with-typescript",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [],
    "rules": {
        "no-unused-vars": 1,
        "unused-imports/no-unused-imports": "warning",
        "no-console": 1
    }
}
